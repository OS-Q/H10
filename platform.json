{
  "name": "P12",
  "title": "Platform P12",
  "description": "Quectel wireless Embedded module",
  "homepage": "http://OS-Q.com/P12",
  "license": "Apache-2.0",
  "keywords": [
    "OS-Q",
    "dev-platform",
    "Quectel",
    "NB-IoT"
  ],
  "engines": {
    "platformio": "^5"
  },
  "version": "1.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/OS-Q/P12.git"
  },
  "packageRepositories": [
    "http://182.61.61.133/link/toolchain/manifest.json",
    "http://182.61.61.133/link/toolchain/openocd.json",
    "http://182.61.61.133/link/framework/manifest.json",
    "http://182.61.61.133/link/platformio.json",
    "http://182.61.61.133/link/platform.json"
  ],
  "frameworks": {
      "opencpu": {
        "package": "framework-quectel",
        "script": "builder/frameworks/opencpu.py"
      },
      "openlinux": {
        "package": "framework-quectel",
        "script": "builder/frameworks/openlinux.py"
      },
      "threadx": {
        "package": "framework-quectel",
        "script": "builder/frameworks/threadx.py"
      },
      "arduino": {
        "package": "framework-quectel",
        "script": "builder/frameworks/arduino.py"
      }
  },
  "packages": {
    "framework-quectel": {
        "type": "framework",
        "version": "https://github.com/Wiz-IO/framework-quectel.git"
    },
    "toolchain-gccarmnoneeabi": {
        "type": "toolchain",
        "version": "~1.70201.0"
    },
    "toolchain-gcc-ec2x": {
        "type": "toolchain",
        "version": "https://github.com/Wiz-IO/LIB/raw/master/quectel/toolchain-gcc-ec2x-v1.0.0.zip"
    },
    "tool-quectel": {
        "type": "uploader",
        "version": "https://github.com/Wiz-IO/tool-quectel.git",
        "optional": true
    }
  }
}
